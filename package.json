{
  "name": "rja-fecit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:backend": "cd backend && poetry run uvicorn main:app --reload",
    "dev:all": "pnpm --parallel --filter \"rja-fecit@*\" --parallel dev",
    "build": "pnpm lint && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:e2e": "playwright test",
    "load-test": "k6 run tests/load.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.15.0",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.21",
    "daisyui": "^4.12.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.0",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "postcss": "^8",
    "prettier": "^3.3.1",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^1.6.0"
  }
}
